<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.smlcx.testset">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".app.ApplicationEx">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.SecondActivity"
            android:launchMode="singleTask"/>
        <activity android:name=".activity.ThirdActivity"
                  android:launchMode="singleTask"/>
        <activity android:name=".activity.LockViewActivity"
                  android:launchMode="singleTask"/>
        <activity android:name=".activity.ToolBarActivity"/>
        <activity android:name=".activity.SwitchActivity"/>
    </application>

</manifest>